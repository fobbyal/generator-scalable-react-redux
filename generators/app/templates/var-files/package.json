{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= desc %>",
  "main": "src/app.js",
  "scripts": {
    "prebuild:prod": "npm run clean-dist",
    "build:prod": "webpack --env.prod -p",
